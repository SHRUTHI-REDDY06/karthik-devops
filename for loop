public class Factorial
{
	public static void main(String[] args)
	{	final int NUM_FACTS = 100;
		for(int in = 0; in < NUM_FACTS; i++)
			System.out.println( in + "! is " + factorial(in));
	}
	
	public static int factorial(int n)
	{	int result = 1;
		for(int in = 2; in <= n; in++)
			result *= in;
		return result;
	}
}
